{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Environment": "Dev",
      "ComponentName": "DDDWithEventSourcingBasedOnMarten"
